{
  "name": "NetUV.Tests",
  "version": "0.1.87",
  "title": "NetUV: Libuv based network library unit tests.",
  "description": "Libuv managed wrapper unit tests",
  "language": "en-US",
  "authors": [
    "Johnny Z"
  ],
  "packOptions": {
    "tags": [ "libuv", "asyncio", "network", "socket", "protocol" ],
    "projectUrl": "https://github.com/StormHub/NetUV",
    "repository": {
      "type": "git",
      "url": "https://github.com/StormHub/NetUV"
    }
  },
  "buildOptions": {
    "keyFile": "../../NetUV.snk",
    "define": [ "SIGNED" ],
    "allowUnsafe": true,
    "copyToOutput": "xunit.runner.json"
  },

  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "NetUV.Core": {
      "target": "project"
    },
    "xunit": "2.2.0-*"
  },

  "testRunner": "xunit",

  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        },
        "System.Net.NetworkInformation": "4.3.0"
      }
    },
    "net461": {
      "dependencies": {
        "System.Net.NetworkInformation": "4.3.0"
      }
    }
  }
}
